# Fordefi API credentials
FORDEFI_API_USER_TOKEN=your_fordefi_api_token

# Fordefi vault (the NEW key owner)
FORDEFI_VAULT_ID=your_fordefi_vault_id
FORDEFI_VAULT_ADDRESS=0x...your_fordefi_vault_aptos_address

# Deployer account (the CURRENT key owner)
DEPLOYER_ADDRESS=0x...your_deployer_aptos_address
DEPLOYER_PRIVATE_KEY_HEX=your_deployer_ed25519_private_key_hex_no_0x_prefix

# Network: mainnet or testnet
APTOS_NETWORK=mainnet
